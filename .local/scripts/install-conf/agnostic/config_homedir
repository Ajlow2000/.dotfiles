#!/bin/sh

# Makes my default file structure

mkdir -p $HOME/downloads
mkdir -p $HOME/media/screenrecordings
mkdir -p $HOME/desktop
mkdir -p $HOME/projects

git clone git@github.com:Ajlow2000/wisc.git $HOME
git clone git@gitlab.com:ajlow2000/records.git $HOME

git clone git@github.com:Ajlow2000/new-languages.git $HOME/projects/
git clone git@github.com:Ajlow2000/unix-tailor.git $HOME/projects/
